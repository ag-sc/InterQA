lex:startOf a lemon:lexicalEntry ;
  lemon:canonicalForm lex:startOf_canonicalForm ;
  lemon:otherForm lex:startOf_otherForm ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:synBehavior lex:startOf_synBehavior ;
